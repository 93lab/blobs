{
  "name": "blobs",
  "version": "1.0.0",
  "description": "svg blob generator",
  "author": "g-harel",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "postpublish": "npm run clean",
    "build": "npm run clean && tsc",
    "clean": "trash '**/*.js' '**/*.js.map' '**/*.d.ts' '!**/node_modules/**/*'",
    "fmt": "prettier --list-different --write --ignore-path .gitignore '**/*.{js,ts}'"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "trash-cli": "^1.4.0",
    "typescript": "^3.1.3"
  },
  "homepage": "https://github.com/g-harel/blobs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g-harel/blobs"
  },
  "bugs": {
    "url": "https://github.com/g-harel/blobs/issues"
  },
  "keywords": [
    "svg",
    "blob"
  ],
  "prettier": {
    "tabWidth": 4,
    "printWidth": 100,
    "trailingComma": "all",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "dependencies": {
    "okwolo": "^3.4.3"
  }
}
